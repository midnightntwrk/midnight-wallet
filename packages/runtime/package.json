{
  "name": "@midnight-ntwrk/wallet-sdk-runtime",
  "description": "Runtime for the wallet variants",
  "version": "1.0.1-rc.0",
  "type": "module",
  "author": "Midnight Foundation",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/midnight-ntwrk/artifacts.git"
  },
  "exports": {
    ".": {
      "development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts"
      },
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./abstractions": {
      "development": {
        "types": "./src/abstractions/index.ts",
        "import": "./src/abstractions/index.ts"
      },
      "types": "./dist/abstractions/index.d.ts",
      "import": "./dist/abstractions/index.js"
    }
  },
  "dependencies": {
    "@midnight-ntwrk/wallet-sdk-abstractions": "2.0.0-rc.0",
    "@midnight-ntwrk/wallet-sdk-utilities": "1.0.1-rc.0",
    "effect": "^3.19.14",
    "rxjs": "^7.5"
  },
  "scripts": {
    "typecheck": "tsc -b ./tsconfig.json --noEmit --force",
    "test": "vitest run",
    "lint": "eslint --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,js,json,yaml,yml,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,yaml,yml,md}\"",
    "dist": "tsc -b ./tsconfig.build.json",
    "dist:publish": "tsc -b ./tsconfig.publish.json",
    "clean": "rimraf --glob dist 'tsconfig.*.tsbuildinfo' && date +%s > .clean-timestamp",
    "publint": "publint --strict"
  },
  "devDependencies": {
    "eslint": "^9.32.0",
    "fast-check": "^4.2.0",
    "prettier": "^3.7.0",
    "publint": "~0.3.14",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
