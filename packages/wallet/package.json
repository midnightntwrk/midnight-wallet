{
  "name": "@midnight-ntwrk/wallet-ts",
  "version": "5.0.0",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./abstractions": {
      "import": "./dist/abstractions/index.mjs",
      "require": "./dist/abstractions/index.cjs",
      "types": "./dist/abstractions/index.d.ts"
    },
    "./effect": {
      "import": "./dist/effect/index.mjs",
      "require": "./dist/effect/index.cjs",
      "types": "./dist/effect/index.d.ts"
    },
    "./v1": {
      "import": "./dist/v1/index.mjs",
      "require": "./dist/v1/index.cjs",
      "types": "./dist/v1/index.d.ts"
    }
  },
  "dependencies": {
    "@midnight-ntwrk/wallet": "workspace:*",
    "@midnight-ntwrk/wallet-api": "^5.0.0",
    "@midnight-ntwrk/wallet-sdk-address-format": "workspace:*",
    "@midnight-ntwrk/zswap": "4.0.0",
    "@scure/base": "^1.1.9",
    "effect": "^3.12.11",
    "isomorphic-ws": "^5.0.0",
    "node-fetch": "3.3.2",
    "rxjs": "^7.5",
    "scale-ts": "^1.1.0",
    "ws": "^8.8.1"
  },
  "scripts": {
    "typecheck": "tsc -b ./tsconfig.json --noEmit",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint",
    "dist": "rollup -c ./rollup.config.mjs"
  }
}
