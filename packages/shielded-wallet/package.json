{
  "name": "@midnight-ntwrk/wallet-sdk-shielded",
  "version": "2.0.0-rc.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Midnight Foundation",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/midnight-ntwrk/artifacts.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./v1": {
      "types": "./dist/v1/index.d.ts",
      "import": "./dist/v1/index.js"
    }
  },
  "dependencies": {
    "@midnight-ntwrk/ledger-v7": "7.0.0",
    "@midnight-ntwrk/wallet-sdk-abstractions": "2.0.0-rc.0",
    "@midnight-ntwrk/wallet-sdk-address-format": "3.0.0",
    "@midnight-ntwrk/wallet-sdk-capabilities": "3.1.0-rc.1",
    "@midnight-ntwrk/wallet-sdk-indexer-client": "1.1.0-rc.1",
    "@midnight-ntwrk/wallet-sdk-node-client": "1.0.0",
    "@midnight-ntwrk/wallet-sdk-prover-client": "1.1.0-rc.1",
    "@midnight-ntwrk/wallet-sdk-runtime": "1.0.1-rc.0",
    "@midnight-ntwrk/wallet-sdk-utilities": "1.0.1-rc.0",
    "@midnight-ntwrk/zkir-v2": "2.1.0",
    "@scure/base": "^1.1.9",
    "effect": "^3.19.14",
    "isomorphic-ws": "^5.0.0",
    "node-fetch": "3.3.2",
    "rxjs": "^7.5",
    "scale-ts": "^1.1.0",
    "ws": "^8.18.3"
  },
  "scripts": {
    "typecheck": "tsc -b ./tsconfig.json --noEmit --force",
    "test": "vitest run",
    "lint": "eslint --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,js,json,yaml,yml,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,yaml,yml,md}\"",
    "dist": "tsc -b ./tsconfig.build.json",
    "dist:publish": "tsc -b ./tsconfig.publish.json",
    "clean": "rimraf --glob dist 'tsconfig.*.tsbuildinfo' && date +%s > .clean-timestamp",
    "publint": "publint --strict"
  },
  "devDependencies": {
    "@effect/cluster": "^0.56.0",
    "@effect/experimental": "^0.58.0",
    "@effect/platform": "^0.94.0",
    "@effect/platform-node": "^0.104.0",
    "@effect/rpc": "^0.73.0",
    "@effect/sql": "^0.49.0",
    "@effect/workflow": "^0.16.0",
    "@polkadot/types": "^16.4.1",
    "eslint": "^9.39.1",
    "fast-check": "^4.4.0",
    "prettier": "^3.8.1",
    "publint": "~0.3.0",
    "rimraf": "^6.0.1",
    "testcontainers": "^11.10.0",
    "to-words": "^4.9.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
