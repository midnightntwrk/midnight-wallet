{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:weekly",
    ":disableRateLimiting"
  ],
  "ignoreDeps": ["rxjs"],
  "nix": {
    "enabled": false
  },
  "packageRules": [
    {
      "matchDatasources": ["sbt-package"],
      "matchPackagePrefixes": ["io.iohk"],
      "enabled": false
    },
    {
      "matchDatasources": ["sbt-package"],
      "matchPackagePrefixes": ["com.github.ghostdogpr"],
      "enabled": false
    }
  ],
  "hostRules": [
    {
      "hostType": "npm",
      "matchHost": "https://npm.pkg.github.com/",
      "encrypted": {
        "token": ""
      },
      "npmrc": "@midnight-ntwrk:registry=https://npm.pkg.github.com/"
    },
    {
      "matchHost": "ghcr.io",
      "hostType": "docker",
      "encrypted": {
        "password": ""
      }
    }
  ]
}
