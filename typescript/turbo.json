{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_AUTH_TOKEN"],
  "tasks": {
    "lint": {
      "outputLogs": "new-only",
      "dependsOn": ["^build"]
    },
    "test-e2e": {
      "cache": false,
      "env": ["NETWORK", "DEPLOYMENT", "SEED", "SEED2", "SEED_STABLE", "ADDRESSES", "NT_SEED", "NT_SEED2"]
    }
  }
}
