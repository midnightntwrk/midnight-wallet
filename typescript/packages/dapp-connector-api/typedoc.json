{
  "out": "docs/api",
  "readme": "README_PUBLIC.md",
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./src/index.ts"
  ],
  "includeVersion": true,
  "disableSources": true,
  "hideGenerator": true,
  "categoryOrder": ["Class", "Interface", "Enum", "Function", "Variable", "*"],
  "categorizeByGroup": true,
  "hideInPageTOC": false,
  "hideBreadcrumbs": false,
  "hidePageTitle": false,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true
}
